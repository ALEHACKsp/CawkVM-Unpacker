<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net48</TargetFramework>
        <Nullable>enable</Nullable>

        <LangVersion>latest</LangVersion>

        <OutputPath>..\$(Configuration)\bin\</OutputPath>
        <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>

        <Product>CawkVM Unpacker</Product>
        <Authors>ElektroKill</Authors>
        <Copyright>Copyright (C) ElektroKill 2021</Copyright>
        <Version>1.0.0.0</Version>
        <InformationalVersion>v1.0.0</InformationalVersion>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="dnlib" Version="3.3.4" />
      <PackageReference Include="Nullable" Version="1.3.0">
        <PrivateAssets>all</PrivateAssets>
        <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      </PackageReference>
    </ItemGroup>

</Project>
